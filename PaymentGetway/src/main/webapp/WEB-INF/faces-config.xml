<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee /WEB-INF/schema/web-facesconfig_2_0.xsd"
	version="2.0" metadata-complete="false">
	<!-- This file must be present with our jar, even if it is empty. Otherwise, 
		our Java annotations won't get scanned! -->
	
	<managed-bean>
		<managed-bean-name>balance</managed-bean-name>
		<managed-bean-class>com.java.payment.Plandetails</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>	
		
	<managed-bean>
		<managed-bean-name>bodyImpl</managed-bean-name>
		<managed-bean-class>com.java.payment.PlandetailsImpl</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>login</managed-bean-name>
		<managed-bean-class>com.java.payment.UserAuth</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>LoginImpl</managed-bean-name>
		<managed-bean-class>com.java.payment.UserAuthImpl</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	
	<managed-bean>
		<managed-bean-name>message</managed-bean-name>
		<managed-bean-class>com.java.payment.Message</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
</faces-config>